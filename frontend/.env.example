# ListingAI Frontend — Environment Variables
# Copy to .env.local and fill in values

# Backend API base URL (required in production)
NEXT_PUBLIC_API_URL=http://localhost:8000

# Default tenant slug for public pages
NEXT_PUBLIC_TENANT_SLUG=galt-ocean-realty

# Sentry error tracking (optional — leave empty to disable)
NEXT_PUBLIC_SENTRY_DSN=
